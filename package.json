    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "css-loader": "0.28.11",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash.uniqueid": "^4.0.1",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.4.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.3.3",
    "react-native-parallax-scroll-opacity": "0.0.2",
    "react-provide-scroll-position": "^1.0.2",
    "react-router-dom": "^4 .3.1",
    "react-scroll": "^1.7.9",
    "react-scroll-parallax": "^1.3.5",
    "react-spark-scroll": "^4.0.0",
    "react-spark-scroll-gsap": "^4.0.0",
    "react-spark-scroll-rekapi": "^4.0.0",
    "react-transition-group": "^2.3.1",
    "react-typist": "^2.0.4",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.4",
    "axios": "latest"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint . --fix"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.10.0",
    "firebase-tools": "^3.18.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
